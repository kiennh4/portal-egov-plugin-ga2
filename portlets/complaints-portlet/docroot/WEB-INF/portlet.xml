<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>complaints</portlet-name>
		<display-name>Complaints Portlet</display-name>
		<portlet-class>
			com.portal_egov.portlet.compaints.ComplaintsPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/view/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/html/view/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>config</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/ComplaintLanguage</resource-bundle>
		<portlet-info>
			<title>Complaints Portlet</title>
			<short-title>Complaints Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>complaints_admin</portlet-name>
		<display-name>Complaints Admin Portlet</display-name>
		<portlet-class>
			com.portal_egov.portlet.compaints.ComplaintsAdminPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/admin/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/ComplaintAdminLanguage</resource-bundle>
		<portlet-info>
			<title>Complaints Admin Portlet</title>
			<short-title>Complaints Admin Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>complaints_category</portlet-name>
		<display-name>Complaints Category Portlet</display-name>
		<portlet-class>
			com.portal_egov.portlet.compaints.ComplaintsCategoryPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/category/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>
			content/ComplaintCategoryLanguage
		</resource-bundle>
		<portlet-info>
			<title>Complaints Category Portlet</title>
			<short-title>Complaints Category Portlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>