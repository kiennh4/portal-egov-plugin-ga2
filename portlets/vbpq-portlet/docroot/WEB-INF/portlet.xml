		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/VBPQCategory_Language</resource-bundle>
		<portlet-info>
			<title>VBPQ Category</title>
			<short-title>VBPQCategoryPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>vbpqCategoryId</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>vbpq_dynamic_menu</portlet-name>
		<display-name>VBPQ Dynamic Menu</display-name>
