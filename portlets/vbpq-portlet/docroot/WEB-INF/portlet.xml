		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>config</portlet-mode>
		</supports>
		<resource-bundle>
			content/VBPQ_Lastest_Entry_Language
		</resource-bundle>
		<portlet-info>
			<title>Vbpq Lastest Entry</title>
