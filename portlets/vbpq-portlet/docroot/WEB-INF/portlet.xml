<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>vbpq</portlet-name>
		<display-name>VBPQ Portlet</display-name>
		<portlet-class>com.portal_egov.portlet.vbpq.VBPQPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/view/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-jsp</name>
			<value>/html/view/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>config</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/VBPQ_Language</resource-bundle>
		<portlet-info>
			<title>VBPQ Portlet</title>
			<short-title>VBPQPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>vbpqEntryId</supported-public-render-parameter>
		<supported-public-render-parameter>vbpqCategoryId</supported-public-render-parameter>
		<supported-public-render-parameter>statisticStartDate</supported-public-render-parameter>
		<supported-public-render-parameter>statisticEndDate</supported-public-render-parameter>
	</portlet>
	
	<portlet>
		<portlet-name>vbpq_management</portlet-name>
		<portlet-class>com.portal_egov.portlet.vbpq.VBPQManagement</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/vbpq_admin/view.jsp</value>
		</init-param>
		<init-param>
			<name>check-auth-token</name>
            <value>false</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/VBPQ_Admin_Language</resource-bundle>
		<portlet-info>
			<title>VBPQ</title>
			<short-title>vbpq-management</short-title>
			<keywords>vbpq-management</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
		
	<portlet>
		<portlet-name>vbpq_category_admin</portlet-name>
