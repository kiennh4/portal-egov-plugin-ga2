	<public-render-parameter>
		<identifier>vbpqEntryId</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:vbpqEntryId</qname>
	</public-render-parameter>
		
	<public-render-parameter>
		<identifier>vbpqCategoryId</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:vbpqCategoryId</qname>
	</public-render-parameter>
		
