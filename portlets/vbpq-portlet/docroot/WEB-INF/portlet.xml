		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>
			content/VBPQ_Dynamic_Menu_Language
		</resource-bundle>
		<portlet-info>
			<title>VBPQ Dynamic Menu</title>
			<short-title>VBPQ Dynamic Menu</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>vbpqCategoryId</supported-public-render-parameter>
