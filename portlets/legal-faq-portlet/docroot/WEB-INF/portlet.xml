<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>legalfaq</portlet-name>
		<display-name>Legal FAQ</display-name>
		<portlet-class>
			com.portal_egov.portlet.legal_faq.LegalFAQ
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/legal_faq_view/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/Legal_FAQ_Language</resource-bundle>
		<portlet-info>
			<title>Legal FAQ</title>
			<short-title>Legal FAQ</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>legalFAQEntryId</supported-public-render-parameter>
		<supported-public-render-parameter>legalFAQCategoryId</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>legal_faq_management</portlet-name>
		<display-name>Legal FAQ Management</display-name>
		<portlet-class>
			com.portal_egov.portlet.legal_faq.LegalFAQManagement
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/legal_faq_management_portlet/management.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content.Legal_FAQ_Admin_Language</resource-bundle>
		<portlet-info>
			<title>Legal FAQ Management</title>
			<short-title>Legal FAQ Management</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>legal_faq_categories</portlet-name>
		<display-name>Legal FAQ Categories</display-name>
		<portlet-class>
			com.portal_egov.portlet.legal_faq.LegalFAQCategories
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/legal_faq_categories_portlet/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-jsp</name>
			<value>/html/legal_faq_categories_portlet/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/Legal_FAQ_Category_Language</resource-bundle>
		<portlet-info>
			<title>Legal FAQ Categories</title>
			<short-title>Legal FAQ Categories</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>legalFAQCategoryId</supported-public-render-parameter>
	</portlet>
	<portlet>
		<portlet-name>legal_faq_lastest_entries</portlet-name>
		<display-name>Legal FAQ Lastest Entries</display-name>
		<portlet-class>
			com.portal_egov.portlet.legal_faq.LegalFAQLastestEntries
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/legal_faq_lastest_entries/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-jsp</name>
			<value>/html/legal_faq_lastest_entries/config.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/Legal_FAQ_Lastest_Language</resource-bundle>
		<portlet-info>
			<title>Legal FAQ Lastest Entries</title>
			<short-title>Legal FAQ Lastest Entries</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-public-render-parameter>legalFAQEntryId</supported-public-render-parameter>
	</portlet>
			
	<public-render-parameter>
		<identifier>legalFAQEntryId</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:legalFAQEntryId</qname>
	</public-render-parameter>
		
	<public-render-parameter>
		<identifier>legalFAQCategoryId</identifier>
		<qname xmlns:x="http://www.liferay.com/public-render-parameters">x:legalFAQCategoryId</qname>
	</public-render-parameter>
</portlet-app>