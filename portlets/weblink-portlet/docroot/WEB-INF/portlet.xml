<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>weblinks</portlet-name>
		<display-name>WebLinks</display-name>
		<portlet-class>
			com.portal_egov.portlet.weblinks.WebLinks
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/weblinks/view.jsp</value>
		</init-param>
		<init-param>
			<name>config-jsp</name>
			<value>/html/weblinks/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/WebLinksLanguage</resource-bundle>
		<portlet-info>
			<title>WebLinks</title>
			<short-title>WebLinks</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>weblinks_admin</portlet-name>
		<display-name>WebLinks Admin</display-name>
		<portlet-class>
			com.portal_egov.portlet.weblinks.WebLinksAdmin
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/weblinks_admin/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<supported-locale>vi_VN</supported-locale>
		<resource-bundle>content/WebLinksAdminLanguage</resource-bundle>
		<portlet-info>
			<title>WebLinks Admin</title>
			<short-title>WebLinksAdmin</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>