<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	
	<portlet>
		<portlet-name>proposition_feedback</portlet-name>
		<icon>/images/icon/proposition_portlet_icon.png</icon>
		<configuration-action-class>com.portal_egov.portlet.proposition_feedback.action.PropositionFeedbackViewConfig</configuration-action-class>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			proposition_feedback-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>proposition_feedback_admin</portlet-name>
		<icon>/images/icon/proposition_feedback_portlet_icon.png</icon>
		<control-panel-entry-category>egov</control-panel-entry-category>
		<control-panel-entry-weight>5.0</control-panel-entry-weight>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			proposition_feedback_admin-portlet
		</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>proposition_feedback_category_admin</portlet-name>
		<icon>/images/icon/proposition_category_portlet_icon.png</icon>
		<control-panel-entry-category>egov</control-panel-entry-category>
		<control-panel-entry-weight>6.0</control-panel-entry-weight>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			proposition_feedback_category_admin-portlet
		</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>lastest_proposation</portlet-name>
		<icon>/images/icon/proposition_portlet_icon.png</icon>
		<configuration-action-class>com.portal_egov.portlet.proposition_feedback.action.LastestPropositionPortletConfig</configuration-action-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			lastest_proposation-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>lastest_proposation_feedback</portlet-name>
		<icon>/images/icon/proposition_feedback_portlet_icon.png</icon>
		<configuration-action-class>com.portal_egov.portlet.proposition_feedback.action.LastestFeedbackPortletConfig</configuration-action-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			lastest_proposation_feedback-portlet
		</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>