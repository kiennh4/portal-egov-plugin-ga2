				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="postName" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.bkav.portal.portlet.employee.model.impl.DepartmentImpl" name="Department">
		<table name="Department" />
		<attributes />
	</entity>
	<entity class="com.bkav.portal.portlet.employee.model.impl.EmployeeImpl" name="Employee">
		<table name="Employee" />
		<attributes />
	</entity>
	<entity class="com.bkav.portal.portlet.employee.model.impl.PostImpl" name="Post">
		<table name="Post" />
		<attributes />
	</entity>
</entity-mappings>